{
  "name": "file-management",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/file-management/src",
  "prefix": "abp",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/packages/file-management"],
      "options": {
        "tsConfig": "packages/file-management/tsconfig.lib.json",
        "project": "packages/file-management/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/file-management/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/file-management/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/file-management"],
      "options": {
        "jestConfig": "packages/file-management/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
