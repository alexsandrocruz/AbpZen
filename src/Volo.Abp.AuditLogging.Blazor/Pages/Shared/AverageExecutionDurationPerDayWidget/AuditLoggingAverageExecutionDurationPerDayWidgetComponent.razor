@using Microsoft.AspNetCore.Authorization
@attribute [Authorize(AbpAuditLoggingPermissions.AuditLogs.Default)]
@inherits AbpAuditLoggingComponentBase

<Card>
    <CardBody>
        <CardTitle Size="5">@L["AverageExecutionDurationInLogsPerDay"]</CardTitle>
        <CardText>
            <div style="position: relative; height: 300px">
                <BarChart @ref="BarChart" TItem="double" Options="@Options"></BarChart>
            </div>
        </CardText>
    </CardBody>
</Card>