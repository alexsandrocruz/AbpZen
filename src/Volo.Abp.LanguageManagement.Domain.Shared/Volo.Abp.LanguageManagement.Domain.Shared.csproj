<Project Sdk="Microsoft.NET.Sdk">

  
  

  <PropertyGroup><LangVersion>latest</LangVersion>
    <TargetFramework>net9.0</TargetFramework>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <Nullable>enable</Nullable>
    <RootNamespace />
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Volo\Abp\LanguageManagement\Localization\Resources\*.json" />
    <Content Remove="Volo\Abp\LanguageManagement\Localization\Resources\*.json" />
    <EmbeddedResource Include="Volo\Abp\LanguageManagement\Localization\ApplicationContracts\*.json" />
    <Content Remove="Volo\Abp\LanguageManagement\Localization\ApplicationContracts\*.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Localization" VersionOverride="9.0.4" />
    <PackageReference Include="Volo.Abp.Validation" VersionOverride="9.0.4" />
    <PackageReference Include="Volo.Abp.Features" VersionOverride="9.0.4" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
  </ItemGroup>
</Project>
