<Project Sdk="Microsoft.NET.Sdk">

  
  

  <PropertyGroup><LangVersion>latest</LangVersion>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net8.0;net9.0</TargetFrameworks>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <RootNamespace />
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Volo\Saas\Localization\Resources\DomainShared\*.json" />
    <Content Remove="Volo\Saas\Localization\Resources\DomainShared\*.json" />
    <EmbeddedResource Include="Volo\Saas\Host\Localization\ApplicationContracts\*.json" />
    <Content Remove="Volo\Saas\Host\Localization\ApplicationContracts\*.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Localization" VersionOverride="9.0.4" />
    <PackageReference Include="Volo.Abp.Validation" VersionOverride="9.0.4" />
    <PackageReference Include="Volo.Abp.FeatureManagement.Domain.Shared" VersionOverride="9.0.4" />
    <PackageReference Include="Volo.Payment.Domain.Shared" VersionOverride="9.0.4" />
  </ItemGroup>

</Project>
