<Project Sdk="Microsoft.NET.Sdk.Web">

    
    

    <PropertyGroup><LangVersion>latest</LangVersion>
        <TargetFramework>net9.0</TargetFramework>
        <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
        <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
        <IsPackable>true</IsPackable>
        <OutputType>Library</OutputType>
        <RootNamespace>Volo.Payment.WeChatPay</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Volo.Abp.Commercial.Core" VersionOverride="9.0.4" />
    
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="BouncyCastle.Cryptography" />
  </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
    
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="BouncyCastle.Cryptography" />
  </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Volo.Payment.WeChatPay.Domain.Shared\Volo.Payment.WeChatPay.Domain.Shared.csproj" />
        <ProjectReference Include="..\Volo.Payment.Web\Volo.Payment.Web.csproj" />
    
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="BouncyCastle.Cryptography" />
  </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="Pages\**\*.css" />
        <EmbeddedResource Include="Pages\**\*.js" />
        <EmbeddedResource Include="Components\**\*.js" />
        <EmbeddedResource Include="Components\**\*.css" />
        <EmbeddedResource Include="Localization\Resources\**\*.json" />
        <Content Remove="Pages\**\*.scss" />
        <Content Remove="Pages\**\*.css" />
        <Content Remove="Pages\**\*.js" />
        <Content Remove="Components\**\*.js" />
        <Content Remove="Components\**\*.css" />
        <Content Remove="Localization\Resources\**\*.json" />
    
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="BouncyCastle.Cryptography" />
  </ItemGroup>

</Project>
